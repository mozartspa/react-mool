{
  "name": "@react-mool/eui",
  "version": "0.1.0",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/lib/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "homepage": "https://github.com/mozartspa/react-mool#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozartspa/react-mool.git"
  },
  "bugs": {
    "url": "https://github.com/mozartspa/react-mool/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "yarn build-esm --watch",
    "build": "rimraf ./dist && yarn build-cjs && yarn build-esm",
    "build-cjs": "rimraf ./dist/lib && tsc",
    "build-esm": "rimraf ./dist/esm && tsc --outDir dist/esm --module es2015",
    "lint": "eslint .",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@react-mool/core": "0.1.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "@react-mool/core": "^0.1.0",
    "react": "^16.9.0 || ^17.0.0"
  },
  "dependencies": {
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^35.1.0",
    "moment": "^2.29.1"
  }
}
